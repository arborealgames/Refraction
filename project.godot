config_version=5

[application]

config/name="Refraction"
run/main_scene="res://scenes/menus/main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

arboreal="*res://global.gd"
global_shader="*res://prefabs/shader.tscn"

[debug]

settings/stdout/print_fps=true
shapes/navigation/geometry_edge_color=Color(0.501961, 1, 1, 0)
shapes/navigation/geometry_face_color=Color(1, 0, 1, 0.4)
shapes/collision/shape_color=Color(1, 0, 1, 0.419608)
shapes/collision/contact_color=Color(1, 0.2, 0.101961, 0.8)

[display]

window/size/viewport_width=256
window/size/viewport_height=144
window/size/initial_position_type=0
window/size/resizable=false
window/size/borderless=true
window/vsync/vsync_mode=0
window/stretch/mode="viewport"

[gui]

theme/custom="res://default_theme.tres"
theme/custom_font="res://font/font0.ttf"
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0
theme/lcd_subpixel_layout=0

